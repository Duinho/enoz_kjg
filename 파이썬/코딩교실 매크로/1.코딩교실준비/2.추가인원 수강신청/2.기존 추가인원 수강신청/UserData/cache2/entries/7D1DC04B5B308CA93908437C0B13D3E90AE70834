		centerMode: true,
		centerPadding: '0px',
 		autoplaySpeed: 3000,
		 customPaging: function (slider, i) {
    var thumb = $(slider.$slides[i]).data();
    if (i == '0') {
      i = "1";
    } else if (i == '1') {
      i = "2";
    } else if (i == '2') {
      i = "3";
    } else if (i == '3') {
      i = "4";
    } else if (i == '4') {
      i = "5";
    } else if (i == '5') {
      i = "6";
    }
    return '<a class="dot"><span>' + i + '</span></a>';
  },
  responsive: [
    {
      breakpoint: 768,
