<script>
    $(document).ready(function () {

		//'ìˆ˜ì—… ë°”ë¡œ ì…ì¥' & 'ìˆ˜ì—…ì…ì¥' ë²„íŠ¼ í´ë¦­ì‹œ ë©”ì„œë“œ ì‹¤í–‰
        $(".enter_class").click(function () {
            $.fn.enterClass();
		});

        //ìˆ˜ì—…ì…ì¥ì‹œ ì‹¤í–‰ë˜ëŠ” ë©”ì„œë“œ
        $.fn.enterClass = function () {
            if (getCookie("MemberId") == "") {
                alert('ë‹¤ì‹œ ë¡œê·¸ì¸í•´ ì£¼ì„¸ìš”.');
                location.href = '/Account/Login?ReturnUrl=' + encodeURI(window.location.pathname);
                return false;
			}

            if ("" == "" || "" == "") {
				alert('ì˜¤ëŠ˜ì€ í™”ìƒì½”ë”© ìˆ˜ì—…ì´ ì—†ìŠµë‹ˆë‹¤.\n\nìˆ˜ì—…ì¼ì •ì„ ë‹¤ì‹œ í™•ì¸í•´ì£¼ì„¸ìš”.');
				location.href = "/MyPage/Study";
				return false;
			};

            var element = $('.enter_class');

            var userid = "";
            var username = "";
			var regdate = "2024-10-30";
            var stime = "";
            var duration = "";

            try {
                //ì…ì¥ê°€ëŠ¥í•œ ì‹œê°„ì¸ì§€ Check
                var jsonData = "{ m_id:\"" + userid + "\", dtSdate:\"" + regdate + "\", stime:\"" + stime + "\", duration:\"" + duration + "\"}";

                $.ajax({
                    url: "/Services/CalendarScript.asmx/GetClassEnterCheck",
                    data: jsonData,
                    async: false,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    type: 'POST',
                    error: function (request, status, error) {
                        alert("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
                    },
                    success: function (data) {
                        if (data.d == "Y") {
                            StartZoom("");
                        }
                        else {
                            alert(data.d);		//"ìˆ˜ì—…ì…ì¥ì´ ê°€ëŠ¥í•œ ì‹œê°„ì´ ì•„ë‹™ë‹ˆë‹¤." ë©”ì‹œì§€ ê²½ê³ ì°½
                        }
                    }
                });
            } catch (err) {
                alert("ê°•ì˜ì‹¤ ì…ì¥ì— ì¥ì• ê°€ ë°œìƒí–ˆìŠµë‹ˆë‹¤. ê³ ê°ì„¼í„°ë¡œ ë¬¸ì˜í•´ì£¼ì„¸ìš”.")
                console.log(err);
            }
        };


	});

    // ì¤Œ ì‹¤í–‰
    function StartZoom(url) {
		window.open(url, "zoom", "width=700,height=700");
	}


    // ì¿ í‚¤ ê°€ì ¸ì˜¤ê¸° í•¨ìˆ˜
    function getCookie(cName) {
        cName = cName + '=';
        var cookieData = document.cookie;
        var start = cookieData.indexOf(cName);
        var cValue = '';
        if (start != -1) {
            start += cName.length;
            var end = cookieData.indexOf(';', start);
            if (end == -1) end = cookieData.length;
            cValue = cookieData.substring(start, end);
        }
        return unescape(cValue);
    }

</script>

