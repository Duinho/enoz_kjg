<script>
    $('.popClick').click(function () {
        $(this).children("div.popWrap").addClass('on')
        $('.popBG').show();
    })
    $('.close-btn').click(function () {
        $('.popWrap').removeClass('on');
        $('.popBG').hide();
    })
</script>

