                                    <script>
                                        function closePop8() {
                                            if (document.getElementById("chkbox8").checked == true) {
                                                setCookie("eventdiv_8", "done", 1);
                                            }
                                            document.getElementById("layer_popup8").style.visibility = "hidden";
                                        }

                                        cookiedata = document.cookie;    

	                                    if ( cookiedata.indexOf("eventdiv_8=done") < 0 ){      
		                                    document.getElementById("layer_popup8").style.visibility = "visible";
	                                    } 
	                                    else {
                                            document.getElementById("layer_popup8").style.visibility = "hidden"; 
	                                    }
                                    </script>
                                  
