<script type="text/javascript">
    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    // ì‹œê°„ ë¦¬ìŠ¤íŠ¸
    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function personCheck() {

        var _val = $(this).val();
        var valTmp = _val.split('_');
        var lecLevel = $("#hidLectLevel_idx").val();
        var timeidx = $("#hidTime_idx").val();
        var target_date = $("#hhdTargetDate").val();
                               
        //var jsonData = JSON.stringify({ jData: jData });

        var jsonData = "{ mode:\"" + "TimeList" + "\", target_date:\"" + target_date + "\", lecture_idx:\"" + valTmp[0] + "\", time_idx:\"" + timeidx + "\", lecture_level_idx:\"" + lecLevel + "\", study_class:\"" + valTmp[1] + "\", user_age:\"11\"}";


        //var time_idx = '<//%=orderModel.iTimeIDX%>';
        $.ajax({
            url: "/Services/OrderScript.asmx/GetTimeList",
            data: jsonData,
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
            success: function (data) {
                var lists = data.d;
                var content = "";

                $.each(lists, function (index, val) {

                    var IDX = val.sIdx;
                    var SEL2 = val.sel2;
                    var RECSS = val.sReCss;
                    var STIME = val.stime;
                    var ETIME = val.etime;
                    var RETXT = val.sReTxt;

                    content += "<option value=\"" + IDX + "\"" + SEL2 + "  " + RECSS + ">" + STIME + " ~ " + ETIME + "&nbsp; &nbsp;" + RETXT + "</option>";
                });
                $("#ddlTimeIDX").empty();
                $(content).appendTo("#ddlTimeIDX");
            },
            error: function (request, status, error) {
                alert("ë‹¤ì‹œ ì‹œë„í•˜ì—¬ ì£¼ì‹­ì‹œì˜¤." + request.responseText);
            }
        });
    }

    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    // ìˆ˜ê°•ì •ë³´ ì‹¤ì œ ì €ì¥
    //////////////////////////////////////////////////////////////////////////////////////////////////////////////

    function OrderUpdate() {

        var oriLect_idx = $("#ddlLectureIDX").val();
        var valTmp = oriLect_idx.split("_");
        var oriTime_idx = $("#ddlTimeIDX").val();
        var timeidx = $("#hidTime_idx").val();

        var ori_book_idx = $("#hidLectLevel_idx").val();
        var ori_lecture_idx = $("#hidLect_idx").val();
        var ori_time_idx = $("#hidTime_idx").val();
        var order_idx = $("#hhdOrderIDX").val();
        var order_no = $("#hhdOrderNo").val();
        var mem_id = $("#hhdMemId").val();
        var lecture_idx = $("#ddlLectureIDX").val();
        var time_idx = $("#ddlTimeIDX").val();

        //var jsonData = JSON.stringify({ jData: jData });

        //string mode, string ori_book_idx, string ori_lecture_idx, string ori_time_idx, string order_idx, string order_no, string mem_id, string lecture_idx, string time_idx
        var jsonData = "{ mode:\"" + "orderUPdate" + "\", ori_book_idx:\"" + ori_book_idx + "\", ori_lecture_idx:\"" + ori_lecture_idx + "\", ori_time_idx:\"" + ori_time_idx + "\", order_idx:\"" + order_idx + "\", order_no:\"" + order_no + "\", mem_id:\"" + mem_id +  "\", lecture_idx:\"" + lecture_idx +  "\", time_idx:\"" + time_idx + "\"}";

        if (oriTime_idx == "" || oriTime_idx == null) {

            alert('ì •ì›ì´ ë§ˆê°ëœ ì‹œê°„ì…ë‹ˆë‹¤.')
            return false;
        }


        if (valTmp[0] != "" && valTmp[0] != null && oriTime_idx != "" && oriTime_idx != null && oriTime_idx != timeidx) {

            $.ajax({
                url: "/Services/OrderScript.asmx/OrderUpdate",
                data: jsonData,
                async: false,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                type: 'POST',
                error: function (request, status, error) {
                    alert("ë‹¤ì‹œ ì‹œë„í•˜ì—¬ ì£¼ì‹­ì‹œì˜¤." + request.responseText);
                }, success: function (data) {

                    var r = data.d;

                    var RESULT = r.iResult;
                    var MESSAGE = r.iMessage;

                    if (RESULT == true) {
                        alert('ë³€ê²½ë˜ì—ˆìŠµë‹ˆë‹¤.');
                        $("#hidLect_idx").val(valTmp[0]);
                        $("#hidTime_idx").val(oriTime_idx);
                        window.location.reload();

                    } else {
                        alert(MESSAGE);
                        //$("#ddlLectureIDX").trigger("change");
                        location.reload();
                    }

                    //$("#ddlTimeIDX").html(data)

                }
            });
        }
    }
</script>

<script>

    $(document).ready(function () {
        $("#flip").click(function () {
            $("#divBank").slideToggle("slow");
        });

        $("#ddlLectureIDX").change(personCheck).change();
        $("#btnSave").click(function () {
            var yesno = confirm("ê°•ì¢Œë¥¼ ë³€ê²½í•˜ì‹œê² ìŠµë‹ˆê¹Œ?");
            if (yesno == false) {
                return;
            } else {
                OrderUpdate();
            }
        });
    });

</script>

