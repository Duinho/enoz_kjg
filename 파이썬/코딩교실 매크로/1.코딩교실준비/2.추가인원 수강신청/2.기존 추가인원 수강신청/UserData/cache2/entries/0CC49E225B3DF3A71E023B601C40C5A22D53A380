                        $('#view_addr').css('display', 'block');
                        $('#edit_addr').css('display', 'none');
                    });

                    $('#btn_ok').click(function () {
						if (!$("#tbOrdName").val()) {
                            alert("К╟⌡К┼■Л┌╛К·▄Л²└ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
                            return;
                        }
                        if (!$("#tbOrdPhone1").val() || !$("#tbOrdPhone2").val() || !$("#tbOrdPhone3").val() || !$("#tbOrdMobile1").val() || !$("#tbOrdMobile2").val() || !$("#tbOrdMobile3").val()) {
                            alert("Л≈╟К²╫Л╡≤К╔╪ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
                            return;
                        }
                        if (!$("#tbOrdZipCode1").val()) {
                            alert("Л ╟М▌╦К╡┬М≤╦К╔╪ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
                            return;
                        }
                        if (!$("#tbOrdAddr1").val()) {
                            alert("Лё╪Л├▄К╔╪ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
                            return;
                        }
                        var order_no = $("#hhdOrderNo2").val();
                        var ord_name = $("#tbOrdName").val();
                        var ord_phone = $("#tbOrdPhone1").val() + '-' + $("#tbOrdPhone2").val() + '-' + $("#tbOrdPhone3").val();
                        var ord_mobile = $("#tbOrdMobile1").val() + '-' + $("#tbOrdMobile2").val() + '-' + $("#tbOrdMobile3").val();
                        var ord_zipcode = $("#tbOrdZipCode1").val();
                        var ord_addr1 = $("#tbOrdAddr1").val();
                        var ord_addr2 = $("#tbOrdAddr2").val();
                        var ord_comment = $("#tbOrdComment").val();

                        var jsonData = "{order_no:\"" + order_no + "\", ord_name:\"" + ord_name + "\", ord_phone:\"" + ord_phone + "\", ord_mobile:\"" + ord_mobile + "\", ord_zipcode:\"" + ord_zipcode + "\", ord_addr1:\"" + ord_addr1 + "\", ord_addr2:\"" + ord_addr2 + "\", ord_comment:\"" + ord_comment + "\"}";

                        $.ajax({
                            url: "/Services/OrderScript.asmx/UpdateOrderBookInfo",
                            data: jsonData,
                            async: false,
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            type: 'POST',
                            success: function (data) {
                                if (data.d == "Y") {
                                    alert('К╟╟Л├║Л╖─ Л═∙КЁ╢Й╟─ КЁ─Й╡╫К░≤Л≈┬Л┼╣К▀┬К▀╓.');
                                    window.location.reload();
                                }
                                else {
                                    alert(data.d);
                                    location.reload();
                                }
                            },
                            error: function (request, status, error) {
                                alert("К▀╓Л▀° Л▀°К▐└М∙≤Л≈╛ Лё╪Л▀╜Л▀°Л≤╓." + request.responseText);
                            }
                        });
                    });
