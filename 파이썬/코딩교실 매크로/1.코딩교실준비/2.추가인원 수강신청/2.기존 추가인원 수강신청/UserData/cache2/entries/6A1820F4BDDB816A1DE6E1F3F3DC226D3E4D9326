//IFRAMEнЋмќґм§Ђ RESIZE
function initFrame(){
	document.result.resizeTo(result.document.body.scrollWidth, result.document.body.scrollHeight);
}

//Enterн‚¤ мќґлІ¤нЉё л§‰кё°
function eventKeyPress()
{
	if (event.keyCode==13)
	{
		this.focus();
	}
}

// Null мІґнЃ¬
function funcIsNull(obj, str, type)
{
	if(obj.value == "")
	{
		var strType = "";
		if(type == "insert")
		{
			strType = "мћ…л Ґ";
		} else if(type == "select") {
			strType = "м„ нѓќ";
		}
		alert(str + " "+ strType +"н•ґ мЈјм„ёмљ”.");
		obj.focus();
		return false;
	}
	else
	{
		return true;	
	}
}

// н•њкёЂмћ…л Ґкё€м§Ђ
function funcNonKr(obj) {
	onvalue = obj.value;
	if (onvalue.search(/[г„±-г…Ћ|г…Џ-г…Ј|к°Ђ-нћќ]/) != -1) {
		alert("мЃл¬ёмќґл‚ м€«мћђл§Њ мћ…л Ґ к°ЂлЉҐн•©л‹€л‹¤.");
		obj.value = "";
		obj.focus();
		return false;
	}
}


