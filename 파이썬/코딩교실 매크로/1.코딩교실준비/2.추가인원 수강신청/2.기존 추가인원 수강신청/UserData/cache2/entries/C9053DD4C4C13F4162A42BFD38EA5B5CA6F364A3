// ì£¼ë¯¼ë“±ë¡ë²ˆí˜¸
function IsValidJumin(val) {
    var pattern = /^([0-9]{6})-?([0-9]{7})$/; 
	var num = val;
    if (!pattern.test(num)) 
		return false; 
    num = RegExp.$1 + RegExp.$2;

	var sum = 0;
	var last = num.charCodeAt(12) - 0x30;
	var bases = "234567892345";
	for (i=0; i<12; i++) {
		if (isNaN(num.substring(i,i+1))) 
			return false;
		sum += (num.charCodeAt(i) - 0x30) * (bases.charCodeAt(i) - 0x30);
	}
	var mod = sum % 11;
	return ((11 - mod) % 10 == last) ? true : false;
}

// ì‚¬ì—…ìë“±ë¡ë²ˆí˜¸
function IsValidBizNum( val )
{
	var replaceVal = val.replace("-","");
					
	var bizID = replaceVal.replace("-","");
	// bizIDëŠ” ìˆ«ìë§Œ 10ìë¦¬ë¡œ í•´ì„œ ë¬¸ìì—´ë¡œ ë„˜ê¸´ë‹¤.
	var checkID = new Array(1, 3, 7, 1, 3, 7, 1, 3, 5, 1);
	var tmpBizID, i, chkSum=0, c2, remander;

	for (i=0; i<=7; i++) chkSum += checkID[i] * bizID.charAt(i);

	c2 = "0" + (checkID[8] * bizID.charAt(8));
	c2 = c2.substring(c2.length - 2, c2.length);

	chkSum += Math.floor(c2.charAt(0)) + Math.floor(c2.charAt(1));

	remander = (10 - (chkSum % 10)) % 10 ;

	if (Math.floor(bizID.charAt(9)) == remander) return true; // OK!
	return false;
}
// ì´ë©”ì¼ í¬ë©§ ì²´í¬
function IsValidFormat(input,format) {
	if (input.value.search(format) != -1) 
	{
		return true; //ok
	}
	//input.focus();
	alert("ì˜ëª»ëœ ì´ë©”ì¼ í˜•ì‹ ì…ë‹ˆë‹¤.");
	input.value = "";
	input.focus();
	return false;
}

// ì´ë©”ì¼ í¬ë©§
function IsValidEmail(input) {
	//    var format = /^(\S+)@(\S+)\.([A-Za-z]+)$/;
	var format = /^((\w|[\-\.])+)@((\w|[\-\.])+)\.([A-Za-z]+)$/;
	return IsValidFormat(input,format);
}

// ì´ë©”ì¼ ì²´í¬
function MailCheck(ObjMail)
{
	if (ObjMail.search(/(\S+)@(\S+)\.(\S+)/) == -1 ) 
	{
		return false;    
	}
	return true;
}


