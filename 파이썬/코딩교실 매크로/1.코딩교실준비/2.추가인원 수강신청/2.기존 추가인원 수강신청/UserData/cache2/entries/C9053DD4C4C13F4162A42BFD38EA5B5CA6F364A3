		alert("ì˜ë¬¸ì´ë‚˜ ìˆ«ìë§Œ ì…ë ¥ ê°€ëŠ¥í•©ë‹ˆë‹¤.");
		obj.value = "";
		obj.focus();
		return false;
	}
}


