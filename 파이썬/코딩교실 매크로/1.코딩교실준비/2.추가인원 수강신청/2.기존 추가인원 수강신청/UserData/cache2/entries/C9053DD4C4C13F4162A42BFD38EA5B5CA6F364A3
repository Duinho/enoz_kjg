}

// Null ì²´í¬
function funcIsNull(obj, str, type)
{
	if(obj.value == "")
	{
		var strType = "";
		if(type == "insert")
		{
			strType = "ì…ë ¥";
		} else if(type == "select") {
			strType = "ì„ íƒ";
		}
		alert(str + " "+ strType +"í•´ ì£¼ì„¸ìš”.");
		obj.focus();
		return false;
	}
	else
	{
		return true;	
	}
}

