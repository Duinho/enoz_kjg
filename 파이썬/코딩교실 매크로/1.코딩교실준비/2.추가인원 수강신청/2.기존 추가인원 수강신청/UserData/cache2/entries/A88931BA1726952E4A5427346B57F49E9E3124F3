<script type="text/javascript">
    // К⌠╠К║² / Л┬≤Л═∙
    function funcSave(msg) {
        var obj = document.frm;

		flag = 0;
        if (obj.rdIDX != undefined) {
            if (obj.rdIDX.length == undefined) {	//Л╡╢М│╛К╟∙Л┼╓Й╟─ М∙≤К┌≤Л²╪Й╡╫Л ╟
                if (obj.rdIDX.checked) {
                    flag = 1;
                }
            } else {
                for (i = 0; i < obj.rdIDX.length; i++) {
                    if (obj.rdIDX[i].checked) {
                        flag = 1;
                    }
                }
            }
        }
        if (flag == 0) {
            alert("Л┬≤Й╟∙Л▀═Л╡╜М∙═ Й╟∙Л╒▄К╔╪ Л└═М┐²М∙╢ Лё╪Л└╦Л ■.");
            return false;
        }

        var yesno = confirm(msg);
        if (yesno == false) {
            return;
        }
        obj.submit();
    }

    function funcSearch() {
        document.frmSearch.submit();
    }

    $(document).ready(function () {
        $(".btn_stime").unbind("click").bind("click", function () {
            $('#stime').val($(this).data('stime'));
            $('#background').css('display', 'block');
            funcSearch();
        });
	});

	$(document).ready(function () {	
		//Й╦╟Л║╢ Л└═М┐²М∙° Й╦╟Л┬≤Й╟▓ Л°═Л╖─
		if ($("#hhdTarget").val() != "") {
			$("input:radio[name='rdTarget']:radio[value='" + $("#hhdTarget").val() +"']").prop('checked', true); // Й╦╟Л║╢ Л└═М┐²М∙° Й╦╟Л┬≤ Л└═М┐²М∙≤Й╦╟         
		}

		$(".btn_week").unbind("click").bind("click", function () {

			if ($('input[name=rdTarget]:checked').val() == undefined) {
				alert("Й╣░Л°║Й╦╟Л┬≤К╔╪ Л└═М┐²М∙╢ Лё╪Л└╦Л ■.");
				return;
			}

			$('#week').val($(this).data('week'));   
            $("#hhdTarget").val($('input[name=rdTarget]:checked').val());	//Л└═М┐²К░° Й╦╟Л┬≤Й╟▓ Л═─Л·╔
            funcSearch();
		});	

		//Й╦╟Л┬≤Л└═М┐² К²╪К■■Л≤╓К╡└М┼╪ КЁ─Й╡╫ Л▀°
        $("input[name='rdTarget']").change(function () {            
			$("#hhdTarget").val($('input[name=rdTarget]:checked').val());	//Л└═М┐²К░° Й╦╟Л┬≤Й╟▓ Л═─Л·╔
			$('#week').val('');	//Л ■Л²╪ Л└═М┐² Л╢┬Й╦╟М≥■

            funcSearch();
        });

        
    });
	//2023.10.10.minhwan.add.К╞╦К·≤Й╣░Л°║Л▀╓ Л ■Л╡╜Л°╪К║° Л╤■Й╟─.
	function course_reg_end(){
		alert("3Й╦╟ Л┬≤Й╟∙Л▀═Л╡╜Л²╢ К╙╗К▒░ К╖┬Й╟░К░≤Л≈┬Л┼╣К▀┬К▀╓. Й╟░Л┌╛М∙╘К▀┬К▀╓");
		location.replace("/");
	}
	//course_reg_end();
