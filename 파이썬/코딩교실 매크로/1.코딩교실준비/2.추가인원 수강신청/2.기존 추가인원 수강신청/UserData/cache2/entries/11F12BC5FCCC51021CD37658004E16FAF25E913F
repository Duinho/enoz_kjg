<script type="text/javascript">
    // л“±лЎќ / м€м •
    function funcSave(msg) {
        var obj = document.frm;

		flag = 0;
        if (obj.rdIDX != undefined) {
            if (obj.rdIDX.length == undefined) {	//мІґнЃ¬л°•мЉ¤к°Ђ н•л‚мќјкІЅмљ°
                if (obj.rdIDX.checked) {
                    flag = 1;
                }
            } else {
                for (i = 0; i < obj.rdIDX.length; i++) {
                    if (obj.rdIDX[i].checked) {
                        flag = 1;
                    }
                }
            }
        }
        if (flag == 0) {
            alert("м€к°•м‹ мІ­н•  к°•мўЊлҐј м„ нѓќн•ґ мЈјм„ёмљ”.");
            return false;
        }

        var yesno = confirm(msg);
        if (yesno == false) {
            return;
        }
        obj.submit();
    }

    function funcSearch() {
        document.frmSearch.submit();
    }

    $(document).ready(function () {
        $(".btn_stime").unbind("click").bind("click", function () {
            $('#stime').val($(this).data('stime'));
            $('#background').css('display', 'block');
            funcSearch();
        });
	});

	$(document).ready(function () {	
		//кё°мЎґ м„ нѓќн•њ кё°м€к°’ мњ м§Ђ
		if ($("#hhdTarget").val() != "") {
			$("input:radio[name='rdTarget']:radio[value='" + $("#hhdTarget").val() +"']").prop('checked', true); // кё°мЎґ м„ нѓќн•њ кё°м€ м„ нѓќн•кё°         
		}

		$(".btn_week").unbind("click").bind("click", function () {

			if ($('input[name=rdTarget]:checked').val() == undefined) {
				alert("кµђмњЎкё°м€лҐј м„ нѓќн•ґ мЈјм„ёмљ”.");
				return;
			}

			$('#week').val($(this).data('week'));   
            $("#hhdTarget").val($('input[name=rdTarget]:checked').val());	//м„ нѓќлђњ кё°м€к°’ м ЂмћҐ
            funcSearch();
		});	

		//кё°м€м„ нѓќ лќјл””м¤лІ„нЉј ліЂкІЅ м‹њ
        $("input[name='rdTarget']").change(function () {            
			$("#hhdTarget").val($('input[name=rdTarget]:checked').val());	//м„ нѓќлђњ кё°м€к°’ м ЂмћҐ
			$('#week').val('');	//мљ”мќј м„ нѓќ мґ€кё°н™”

            funcSearch();
        });

        
    });
