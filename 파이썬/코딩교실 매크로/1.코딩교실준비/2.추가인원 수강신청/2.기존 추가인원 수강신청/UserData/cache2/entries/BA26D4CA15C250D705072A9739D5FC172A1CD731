	//ì¼ì¼í‰ê°€ì„œ
    function popDailyReport(group_no, order_no, study_day) {
        var url = '/Mypage/PopDailyReport?group_no=' + group_no + "&order_no=" + order_no + "&study_day=" + study_day;
        window.open(url, "Report", "left=200, top=100, toolbar=0, scrollbars=yes, resizable=Yes, width=680, height=720");
    }
	//ì›”ë§í‰ê°€ì„œ
    function popMonthlyReport(idx) {
        var url = '/Mypage/PopMonthlyReport?idx=' + idx;
        window.open(url, "ReportMon", "left=200, top=100, toolbar=0, scrollbars=yes, resizable=Yes, width=680, height=800");
    }
</script>

