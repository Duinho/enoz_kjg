        $.ajax({
            url: "/Services/CalendarScript.asmx/GetCalendarOff",
            data: jsonData,
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
            error: function (request, status, error) {
                alert("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
            },
            success: function (data) {
                lists = data.d;
                $.each(lists, function (index, val) {
                    var _date = val.date;
                    _date = _date.substring(5, 10);
                    _date = _date.replace('-', '');
                    var _text = val.text;

                    content = "<div class=\"no\">íœ´ì¼<div>" + _text + "</div></div>";
                    $('.inner.date' + _date).html(content);
                    $('.day.date' + _date).siblings().remove();
                    $('.day.date' + _date).after(content);
                });
            }
        });
    });

