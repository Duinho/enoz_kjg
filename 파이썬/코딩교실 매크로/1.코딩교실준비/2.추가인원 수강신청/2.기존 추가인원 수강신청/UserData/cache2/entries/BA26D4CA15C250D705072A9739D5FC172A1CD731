    //ê²°ì„ì˜ˆì • ë“±ë¡
    function SubmitAbsence(m_group, m_id, group_no, order_no, teacher_name, study_day) {
        if (!confirm("ê²°ì„ ì˜ˆì •ì„ ë“±ë¡í•˜ì‹œë©´ ì·¨ì†Œê°€ ë¶ˆê°€í•˜ì˜¤ë‹ˆ ì‹ ì¤‘í•˜ê²Œ ê²°ì •í•´ì£¼ì„¸ìš”.\ní•´ë‹¹ì¼ìì˜ ê²°ì„ì˜ˆì •ì„ ìš”ì²­í•˜ì‹œê² ìŠµë‹ˆê¹Œ?")) {
            return false;
        }

        var obj = new Object();
        obj.m_group = m_group;
        obj.m_id = m_id;
        obj.group_no = group_no;
        obj.order_no = order_no;
        obj.teacher_name = teacher_name;
        obj.study_day = study_day;
        var jsonData = JSON.stringify(obj);

        $.ajax({
            url: "/Services/CalendarScript.asmx/SubmitAbsence",
            data: jsonData,
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
            error: function (request, status, error) {
                alert("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
            },
            success: function (data) {
                if (data.d == "Y") {
                    alert("ê²°ì„ì˜ˆì • ë“±ë¡ì´ ì™„ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.");
                    location.reload();
                }
                else {
                    alert("ê²°ì„ì˜ˆì • ë“±ë¡ì— ë¬¸ì œê°€ ë°œìƒí–ˆìŠµë‹ˆë‹¤.\nì„¼í„°ë¡œ ë¬¸ì˜ë°”ëë‹ˆë‹¤.");
                    console.log(data.d);
                }
            }
        });
    }
	// ì¿ í‚¤ ê°€ì ¸ì˜¤ê¸° í•¨ìˆ˜
    function getCookie(cName) {
        cName = cName + '=';
        var cookieData = document.cookie;
        var start = cookieData.indexOf(cName);
        var cValue = '';
        if (start != -1) {
            start += cName.length;
            var end = cookieData.indexOf(';', start);
            if (end == -1) end = cookieData.length;
            cValue = cookieData.substring(start, end);
        }
        return unescape(cValue);
    }
