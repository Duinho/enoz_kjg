        $(".on_video").click(function () {
            var _roomcode = $(this).data("roomcode");
            var _username = $(this).data("username");
            var _userid = $(this).data("userid");
            var _ver = $(this).data("ver");
            var _tcode = $(this).data("tcode");

            $.ajax({
                url: "/Services/CalendarScript.asmx/GetTestRoomCheck",
                async: false,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                type: 'POST',
                error: function (request, status, error) {
                    alert("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
                },
                success: function (data) {
                    var lists = data.d;

                    var AllowChk = lists.AllowChk;
                    var StartTime = lists.StartTime;
                    var EndTime = lists.EndTime;

                    if (AllowChk == "Y") {
                        StartBoda(_userid, _username, _roomcode, _tcode);
                    }
                    else if (AllowChk == "N") {
                        alert(StartTime + ' ~ ' + EndTime + ' ê¹Œì§€ í…ŒìŠ¤íŠ¸ê°•ì˜ì‹¤ ì´ìš©ì´ ë¶ˆê°€í•©ë‹ˆë‹¤.\n\nì´ìš©ì— ë¶ˆí¸ë“œë ¤ ì£„ì†¡í•©ë‹ˆë‹¤.');
                    }
                }
            });

