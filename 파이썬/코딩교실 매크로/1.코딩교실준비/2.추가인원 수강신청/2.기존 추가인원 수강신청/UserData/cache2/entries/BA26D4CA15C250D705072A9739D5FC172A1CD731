    // í™”ìƒì†”ë£¨ì…˜ 'ì‹ 'ë²„ì „ API Start
    function StartBoda(_userid, _username, _roomcode, _tcode) {

        MvApi.openJoin({
            'roomCode': '003225' + 'K' + _roomcode,
            'template': _tcode,
            'title': _roomcode,
            'openOption': 0,
            'joinUserType': 12,	//í•™ìƒ
            'userId': _userid,
            'userName': _username,
            'roomOption': '',
            'extraMsg': ''
        }, function () {
            console.log("[ì˜¨ë¼ì¸ SW ì½”ë”© êµì‹¤] í™”ìƒ ì ‘ì† ì„±ê³µ");
        }, function (errorCode, reason) {
            var msg = errorCode + ": " + reason;
            console.log(msg);
        });
    }

