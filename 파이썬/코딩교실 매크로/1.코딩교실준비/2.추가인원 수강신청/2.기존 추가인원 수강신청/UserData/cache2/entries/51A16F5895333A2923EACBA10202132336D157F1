<!-- 팝업 "하루동안 열지않기" 쿠키생성 -->
<script>
    function setCookie(name, value, expiredays) {
        var todayDate = new Date();
        todayDate.setDate(todayDate.getDate() + expiredays);
        document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
	}
</script>

<script>

    $(document).ready(function () {
        //'테스트강의실' 클릭시 호출
        $(".on_video").click(function () {
            // 테스트 강의실 일시 중지 시킴
            //alert("현재 수업기간이 아닙니다");
            //return false;


            var _roomcode = $(this).data("roomcode");
            var _username = $(this).data("username");
            var _userid = $(this).data("userid");
            var _ver = $(this).data("ver");
            var _tcode = $(this).data("tcode");
            var _link = $(this).data("link");

            $.ajax({
                url: "/Services/CalendarScript.asmx/GetTestRoomCheck",
                async: false,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                type: 'POST',
                error: function (request, status, error) {
                    alert("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
                },
                success: function (data) {
                    var lists = data.d;

                    var AllowChk = lists.AllowChk;
                    var StartTime = lists.StartTime;
                    var EndTime = lists.EndTime;

                    if (AllowChk == "Y") {
                        //StartBoda(_userid, _username, _roomcode, _tcode);
                        StartZoom(_link);
                    }
                    else if (AllowChk == "N") {
                        alert(StartTime + ' ~ ' + EndTime + ' 까지 테스트강의실 이용이 불가합니다.\n\n이용에 불편드려 죄송합니다.');
                    }
                }
            });

        });


    });
</script>


