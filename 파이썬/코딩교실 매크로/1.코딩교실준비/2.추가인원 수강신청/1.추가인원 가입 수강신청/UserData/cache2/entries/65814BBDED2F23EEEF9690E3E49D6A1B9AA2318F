(function ($) {
    
    var gnb, gnbLayout, gnbScroll, changeTop, dot, dotLayout, dotRange, familySite, skrollrLayout, portfolioBtn,
        isDot = false;
    
    
    addDot = function(){
        //ÁïôÎ®ØÏ®™ÔøΩÍæ™Î™¥ ÔøΩÎçâÏî†ÔøΩÍæ©ÏçêÊÑøÔøΩÔøΩÔøΩ
        /*if( $(".newsList").length >= 1){ //ÔøΩÎåÅÎí™Á¥ê‚ë∏Ï§â ËπÇÎ™É–¶ html tag ÔøΩÏíìÍµÖ
