		                                    //$("#layer_popup .img img").attr("src","/images/main/mPopup"+ tab_num +".jpg")
	                                    })
                                        $("#layer_popup7").draggable({
                                            cursor: "move",
                                            opacity: 0.8
                                        });
                                    </script>

                                    <script>
                                        function closePop7() {
                                            if (document.getElementById("chkbox7").checked == true) {
                                                setCookie("eventdiv_7", "done", 1);
                                            }
                                            document.getElementById("layer_popup7").style.visibility = "hidden";
                                        }

                                        cookiedata = document.cookie;    

	                                    if ( cookiedata.indexOf("eventdiv_7=done") < 0 ){      
		                                    document.getElementById("layer_popup7").style.visibility = "visible";
	                                    } 
	                                    else {
                                            document.getElementById("layer_popup7").style.visibility = "hidden"; 
	                                    }
                                    </script>
                                 

