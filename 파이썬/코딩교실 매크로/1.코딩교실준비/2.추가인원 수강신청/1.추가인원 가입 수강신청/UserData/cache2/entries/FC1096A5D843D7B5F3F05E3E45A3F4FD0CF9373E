				<script>
                    function funcMemCheck() {
                        var obj = $("#tbMemID").val();

                        if (obj == "") {
                            alert("아이디를 입력해 주세요.");
                            $('#tbMemID').focus();
                            return;
                        }

                        var RegExp = /^[A-za-z0-9]{4,15}$/g;  //영문 대문자 또는 소문자 또는 숫자, 길이는 4-15자
                        if (!RegExp.test(obj)) {
                            alert('영문 대/소문자 또는 숫자, 길이는 4-15자로 입력해주세요.');
                            $('#tbMemID').val("");
                            $('#tbMemID').focus();
                            return;
                        }

                        var filter = ['test', 'admin', 'super', 'insert', 'delete', 'dump'];
                        for (var i in filter) {
                            if (obj.match(filter[i])) {
                                alert('금지어가 포함되어있습니다. - ' + filter[i]);
                                $('#tbMemID').val("");
                                $('#tbMemID').focus();
                                return;
                            }
                        }

                        $.ajax({
                            type: "post",
                            url: "/Services/AccountScript.asmx/GetMemberId",
                            data: "{ id:\"" + $("#tbMemID").val() + "\"}",
                            contentType: "application/json; charset=utf-8",
                            dataType: 'json',
                            error: function (request, status, error) {
                                console.log("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
                                alert("Id 조회에 문제가 발생했습니다. 관리자에게 문의 바랍니다.");
                            },
                            success: function (data) {
                                //$("#id_check").text(data);
                                if (data.d == "N") {
                                    $('#tbMemID').val("");
                                    $('#tbMemID').focus();
                                    $("#id_check").html("<font color='red'>중복된 아이디입니다. 다시 확인해 주세요.</font>");
                                } else {
                                    $('#hhdIdCheck').val('Y');
                                    $("#id_check").html("<font color='blue'>사용 가능한 아이디입니다.</font>");
                                }
                            }
                        });
                    }
                </script>

			
			</td>
		</tr>
