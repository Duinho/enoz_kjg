<script type="text/javascript">
    $(document).ready(function () {
		

	})

	$('.btn_zip').click(function () {
		$('#wrap_daum').children('div').css('min-width', '100px');
    });

    $('.IsNum').keyup(function () {
        $(this).val($(this).val().replace(/[^0-9]/gi, "")); //숫자만 입력가능
	});

    $('.IsKor').keyup(function (obj) {
        var reg = /[a-z0-9]|[ \[\]{}()<>?|`~!@#$%^&*-_+=,.;:\"'\\]/g;
        var v = $(this).val();
        var tag = "<span class='dot'>*</span> 한글만 입력 가능합니다.";

        if (reg.test(v)) {
            $(this).val(v.replace(reg, ""));
            $('#tbMemNameAlert').html(tag);
        }
    });
</script>

