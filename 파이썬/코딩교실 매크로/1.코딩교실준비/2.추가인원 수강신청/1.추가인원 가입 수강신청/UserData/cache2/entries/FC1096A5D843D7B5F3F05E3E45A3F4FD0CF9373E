<script>

	function funcEnterID(i) {
		if (event.keyCode == 13 && i == 7) {
			chkUserId();
		}
	}

	// 등록 / 수정
	function funcSave() {
		var obj = document.frm;
		var mode = document.getElementById("mode").value;
		//alert(mode);

		if (mode == "Create") {

			if (!funcIsNull(obj.tbMemName, "이름을", "insert")) return;
			if (!funcIsNull(obj.tbMemID, "아이디를", "insert")) return;
			if (obj.tbMemID.value.length < 4) {
				alert("사용할 아이디를 4자 이상으로 입력하세요.");
				obj.tbMemID.focus();
				return;
			}
			if (obj.hhdIdCheck.value == "") {
				alert("아이디 중복체크를 해주세요.");
				//chkUserId();
				return;
			}
			
			if(obj.tbMemName.value == obj.tbPName.value){
				alert("학생명과 학부모명이 동일합니다.");
				return;
			}

			if (!funcIsNull(obj.tbPName, "학부모 이름을", "insert")) return;

			if (!funcIsNull(obj.tbPMobile1, "학부모 연락처를", "insert")) return;
			if (!funcIsNull(obj.tbPMobile2, "학부모 연락처를", "insert")) return;
			if (!funcIsNull(obj.tbPMobile3, "학부모 연락처를", "insert")) return;

		}

		// ### 비밀번호 유효성 체크 #####
		// 1. 비밀번호는 8자리 이상
		// 2. 비밀번호가 10자리 미만이라면 영문+숫자+특수문자를 포함하여야만 한다.
		// 3. 비밀번호가 10자리 이상이라면 영문, 숫자, 특수문자 중 2가지만 사용해도 된다.
		var pwd1 = document.frm.tbMemPass;
		var inNum = false;
		var inEng = false;
		var inSChar = false;
		var inRange = false;

		if (!pwd1.value) {
			alert("비밀번호를 입력해주세요.");
			pwd1.focus();
			return;
		} else {
			//비밀번호 8자 이상인지 확인
			if (pwd1.value.length < 8) {
				alert("비밀번호는 8자 이상 입력해주세요.");
				pwd1.focus();
				return;
			}
			//숫자포함여부 확인
			for (i = 0; i < pwd1.value.length; i++) {
				if ((pwd1.value.charCodeAt(i) >= 48) && (pwd1.value.charCodeAt(i) <= 57)) {
					inNum = true;
					break;
				}
			}
			//영문자 포함여부 확인
			for (i = 0; i < pwd1.value.length; i++) {
				if (((pwd1.value.charCodeAt(i) >= 65) && (pwd1.value.charCodeAt(i) <= 90)) || ((pwd1.value.charCodeAt(i) >= 97) && (pwd1.value.charCodeAt(i) <= 122))) {
					inEng = true;
					break;
				}
			}
			//특수문자 포함여부 확인
			for (i = 0; i < pwd1.value.length; i++) {
				if (((pwd1.value.charCodeAt(i) >= 33) && (pwd1.value.charCodeAt(i) <= 47)) || ((pwd1.value.charCodeAt(i) >= 58) && (pwd1.value.charCodeAt(i) <= 64)) || ((pwd1.value.charCodeAt(i) >= 91) && (pwd1.value.charCodeAt(i) <= 96))) {
					inSChar = true;
					break;
				}
			}

			if (pwd1.value.length < 10) {    //비밀번호가 10자리 미만이라면
				if (!(inNum && inEng && inSChar)) {
					alert("비밀번호는 영문 대·소문자와 숫자와 특수문자도 함께 사용해 주세요.");
					return;
				}
			} else {
				if ((inNum && inEng) || (inEng && inSChar) || (inNum && inSChar)) {
					//return true;
				} else {
					alert("비밀번호는 영문 대·소문자, 숫자, 특수문자중 2종을 섞어서 사용해 주세요.");
					return;
				}
			}
		}

		// ### 비밀번호 유효성 체크 ###
		if (obj.tbMemPass.value != obj.tbMemPass2.value) {
			alert("비밀번호가 일치하지 않습니다. 동일한 비밀번호를 입력해 주세요.");
			obj.tbMemPass2.focus();
			return;
		}
		// if (!funcIsNull(obj.tbMemEName, "영어 이름을", "insert")) return;
		if (!funcIsNull(obj.tbMobile1, "휴대전화를", "insert")) return;
		if (!funcIsNull(obj.tbMobile2, "휴대전화를", "insert")) return;
		if (!funcIsNull(obj.tbMobile3, "휴대전화를", "insert")) return;

		if (obj.tbMobile1.value == "010"){
			if(obj.tbMobile2.value.length < 4){
				alert("휴대전화 번호를 정확히 입력해주세요");
				obj.tbMobile2.focus();
				return;
			}
			if(obj.tbMobile3.value.length < 4){
				alert("휴대전화 번호를 정확히 입력해주세요");
				obj.tbMobile3.focus();
				return;
			}
		}

		if (obj.tbMemEmail.value != "") {
			if (MailCheck(obj.tbMemEmail.value) == false) {
				alert('잘못된 이메일주소 입니다.');
				obj.tbMemEmail.focus();
				return;
			}
		}

		if (!funcIsNull(obj.ddlBirthDay1, "출생년도를", "select")) return;
		if (!funcIsNull(obj.ddlBirthDay2, "생일을", "select")) return;
		if (!funcIsNull(obj.ddlBirthDay3, "생일을", "select")) return;

		if (!funcIsNull(obj.ddlSex, "자신의 성별을", "select")) return;


		if (!funcIsNull(obj.ddlAcaType, "학교 레벨을", "select")) return;
		if (obj.ddlAcaType.value == "1") {
			if (!funcIsNull(obj.ddlAcaName1, "초등학교 이름을", "select")) return;
			if (!funcIsNull(obj.tbGrade1, "현재 학년을", "insert")) return;
			if (!funcIsNull(obj.tbClass1, "현재 반을", "insert")) return;
		} else if (obj.ddlAcaType.value == "2") {
			if (!funcIsNull(obj.ddlAcaName2, "중학교 이름을", "select")) return;
			if (!funcIsNull(obj.tbGrade2, "현재 학년을", "insert")) return;
			if (!funcIsNull(obj.tbClass2, "현재 반을", "insert")) return;
		} else if (obj.ddlAcaType.value == "3") {
			if (!funcIsNull(obj.ddlAcaName3, "고등학교 이름을", "select")) return;
			if (!funcIsNull(obj.tbGrade3, "현재 학년을", "insert")) return;
			if (!funcIsNull(obj.tbClass3, "현재 반을", "insert")) return;
		} else if (obj.ddlAcaType.value == "5") {
			if (!funcIsNull(obj.tbAcaName5, "관외 학교 이름을", "insert")) return;
			if (!funcIsNull(obj.tbGrade5, "현재 학년을", "insert")) return;
			if (!funcIsNull(obj.tbClass5, "현재 반을", "insert")) return;
		}

		/*
		var isAge = funcAgeCheck(obj);		// 출생년도 학교 레벨 매칭	
		if(!isAge)
			return false;
		*/

		if (!funcIsNull(obj.tbZipCode, "우편번호를", "insert")) return;
		if (!funcIsNull(obj.tbAddr1, "주소를", "insert")) return;
		if( !funcIsNull(obj.tbAddr2, "상세 주소를", "insert") ) return;

		var msg;
		
			msg = "가입";
		
		var yesno = confirm("회원 "+ msg +"하시겠습니까?");
		if( yesno == false ) {
			return;
		}

		obj.submit();
	}

	// 비밀번호 확인 일치여부
	function funcPassConfirm() {
		var obj = document.frm;

		if( obj.tbMemPass.value == obj.tbMemPass2.value ) {
			$("#tbPassConfirm").html("<font color='blue'>* 두 비밀번호가 동일합니다.</font>");
		} else {
			$("#tbPassConfirm").html("<font color='red'>* 두 비밀번호가 동일하지 않습니다.</font>");
		}
	}

	//학년레벨 2
	function funcAcaType() {
		var obj = document.frm;
		if (obj.ddlAcaType.value == 1) {
			$( '#aca_type1' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type1' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 2) {
			$( '#aca_type2' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type2' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 3) {
			$( '#aca_type3' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type3' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 4) {
			$( '#aca_type4' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type4' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 5) {
			$( '#aca_type5' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type5' ).css( 'display', 'none');
		}
	}

	function funcAgeCheck(obj){
		var _year = parseInt(new Date().getFullYear());
		var _birthday1 = parseInt(obj.ddlBirthDay1.value);		
		var _age = _year - _birthday1;

		if((_age >= 10)){
			if(obj.ddlAcaType.value == "1"){
				if(!(_age >= 7 && _age <= 12)){
					alert("출생년도와 학교레벨이 맞지 않습니다. 확인해주세요.");
					obj.ddlAcaType.focus();
					return false;				
				}
			}else if (obj.ddlAcaType.value == "2"){
				if(!(_age >= 13 && _age <= 15)){
					alert("출생년도와 학교레벨이 맞지 않습니다. 확인해주세요.");
					obj.ddlAcaType.focus();
					return false;				
				}
			}else if (obj.ddlAcaType.value == "3"){
				if(!(_age >= 16 && _age <= 18)){
					alert("출생년도와 학교레벨이 맞지 않습니다. 확인해주세요.");
					obj.ddlAcaType.focus();
					return false;				
				}
			}else if (obj.ddlAcaType.value == "4"){
				if(_age >= 7 && _age <= 18){
					alert("출생년도와 학교레벨이 맞지 않습니다. 확인해주세요.");
					obj.ddlAcaType.focus();
					return false;				
				}
			}
		}
		else
		{
			alert("초등학교 4학년 부터 회원가입이 가능합니다.")
		}
		return true;
	}

</script>

<script>
	/* 아이디 : 한글,특수문자 제거 */
	function checkID(obj) {
		var RegExp1 = /^[a-zA-Z0-9]*$/;
		if (!RegExp1.test(obj.value)) {
			obj.value = obj.value.substring(0, obj.value.length - 1);
			$("#id_check").html("<font color='blue'>최소 4자, 영문+숫자 형태로 입력해주세요.</font>");
			$('#hhdIdCheck').val('');
		}
		else {
			$("#id_check").html("<span class='dot'>*</span> <span style='color:blue'>최소 4자, 영문+숫자 형태로 입력해주세요.</span>");
			$('#hhdIdCheck').val('');
		}
	}

	/* 영어이름 : 한글,특수문자 제거 */
	function checkEngName(obj) {
		var RegExp1 = /^[a-zA-Z0-9]*$/;
		var RegExp2 = /[_]/gi;
		if ((RegExp1.test(obj.value) || RegExp2.test(obj.value)) == false) {
			obj.value = obj.value.substring(0, obj.value.length - 1);
			$("#EngName_check").html("<span style='color:blue'>영문으로 입력해주세요.</span>");
		}
	}
</script>

