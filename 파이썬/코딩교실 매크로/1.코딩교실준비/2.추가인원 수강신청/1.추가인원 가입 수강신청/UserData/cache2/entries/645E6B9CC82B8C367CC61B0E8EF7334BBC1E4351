<script type="text/javascript">
    function funcAgree() {
        //alert("화상코딩 회원가입이 중지 되었습니다. 감사합니다.");

        var obj = document.frm;
        if (obj.cbAgree1.checked == false) {
            alert("회원 약관에 동의 하세요.");
            return;
        }
        if (obj.cbAgree2.checked == false) {
            alert("개인정보 취급방침에 동의 하세요.");
            return;
        }
        obj.submit();
    }
</script>

