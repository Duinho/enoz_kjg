<script>
	$(document).ready(function(){	
		$('.popup .popupup form .madal-btn .close').click(function () {
			$('#modal').fadeOut();
		});

		$("#btn_poll").click(function () {
			var frm_poll = document.form_poll;
			if (frm_poll.q1.value == "" || frm_poll.q2.value == "" || frm_poll.q3.value == "" || frm_poll.q4.value == "" || frm_poll.q5.value == "") {
				alert("조사 항목을 선택해주세요.");
				return false;
            }
            $("#txtPoll_1").val(frm_poll.q1.value);
            $("#txtPoll_2").val(frm_poll.q2.value);
            $("#txtPoll_3").val(frm_poll.q3.value);
            $("#txtPoll_4").val(frm_poll.q4.value);
			$("#txtPoll_5").val(frm_poll.q5.value);

            $('#modal').fadeOut();
		});
	});
</script> 
