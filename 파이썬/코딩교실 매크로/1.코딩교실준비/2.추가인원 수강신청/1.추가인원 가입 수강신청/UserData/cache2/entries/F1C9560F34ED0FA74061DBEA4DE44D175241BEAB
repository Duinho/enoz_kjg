<script>

	// 여분필드 value string 생성
    function makeParameter() {

        var _id = $("#UserId").val();
        var _low_tmp = $("#hhdLow").val();
        var _jse = $("#hhdCheckJS").val();
		var _bookprice = $("#BookAmt").val();
        var _book = $("#hhdBook").val();
        var _workbook = $("#hhdWorkBook").val();
        var _level_idx = $("#hhdLevelIDX").val();
        var _lecture_idx = $("#hhdLecIDX").val();
		var _time_idx = $("#hhdTimeIDX").val();
        var _lec_time = $("#hhdLecTime").val();

        var _ord_name = $("#tbOrdName").val();
        var _ord_phone = $("#tbOrdPhone1").val() + '-' + $("#tbOrdPhone2").val() + '-' + $("#tbOrdPhone3").val();
        var _ord_mobile = $("#tbOrdMobile1").val() + '-' + $("#tbOrdMobile2").val() + '-' + $("#tbOrdMobile3").val();
        var _ord_zipcode = $("#tbOrdZipCode1").val();
        var _ord_add1 = $("#tbOrdAddr1").val();
        var _ord_add2 = $("#tbOrdAddr2").val();
        var _ord_comment = $("#tbOrdComment").val();

        var jsonData = "{ id:\"" + _id + "\", low_tmp:\"" + _low_tmp + "\", jse:\"" + _jse + "\", bookprice:\"" + _bookprice + "\", book:\"" + _book
            + "\", workbook:\"" + _workbook + "\", level_idx:\"" + _level_idx + "\", lecture_idx:\"" + _lecture_idx + "\", time_idx:\"" + _time_idx + "\", lec_time:\"" + _lec_time
					+ "\", ord_name:\"" + _ord_name + "\", ord_phone:\"" + _ord_phone + "\", ord_mobile:\"" + _ord_mobile + "\", ord_zipcode:\"" + _ord_zipcode
					+ "\", ord_add1:\"" + _ord_add1 + "\", ord_add2:\"" + _ord_add2 + "\", ord_comment:\"" + _ord_comment + "\"}";
		//alert(jsonData);
        $.ajax({
            url: "/Services/OrderScript.asmx/MakeParameterString",
            data: jsonData,
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
			success: function (data) {
                var r = data.d;
                $("#ReqReserved").val(r);
            },
            error: function (request, status, error) {
                alert("다시 시도하여 주십시오." + request.responseText);
            }
        });
    }

    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    // 잔여인원체크
    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function personCheck(gubun, method) {

		//var isCbDevice = "";
		//$("input[name=cbDevice]:checked").each(function(){
		//	isCbDevice = "Y";
		//});

		//if(isCbDevice != "Y") {
		//	alert("교육 장비 구비 여부는 한 개 이상 선택해주세요.");
		//	$("#cbPc").focus();
		//	return false;
		//}


        var time_idx = $("#hhdTimeIDX").val();
        $.ajax({
            url: "/Services/OrderScript.asmx/PersonCheck",
            data: "{ mode:\"Person" + "\", selectedId:\"" + time_idx + "\"}",
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
            success: function (data) {
                var r = data.d;
                var RESULT = r.iResult;
                var ReCnt = r.iReCnt;
                // 잔여인원이 남아 았을경우
                if (parseInt(ReCnt) >= 0 && RESULT == true) {
                    console.log('잔여인원 Check OK');
                    if (gubun == 0) {
                        pay_info_cash();
                    }
                    else {
                        requestPayment(gubun, method);
                    }
                }
                else {	// 마감되었을 경우 
                    var txt = "죄송합니다.\n신청하신 수업은 정원이 마감되었습니다.\n강좌를 취소하시겠습니까?";
                    orderAppCancel(1987, txt);
                }
            },
            error: function (request, status, error) {
                alert("다시 시도하여 주십시오." + request.responseText);
            }
        });
    }

    function orderAppCancel(idx, txt) {
        if (confirm(txt)) {
            $.ajax({
                url: "/Services/OrderScript.asmx/CourseApplicationCancel",
                data: "{ idx:\"" + idx + "\"}",
                async: false,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                type: 'POST',
                success: function (data) {
                    var r = data.d;
                    if (r == "Y") {
                        alert("취소되었습니다.");
                        location.href = "/";
                    }
                    else {
                        alert("오류가 발생되었습니다.");
                        location.href = "/";
                    }
                },
                error: function (request, status, error) {
                    alert("다시 시도하여 주십시오." + request.responseText);
                }
            });
        }
        return false;
	}

    function kakaopayStart() {
        console.log('KakaoPay Start');
        //alert(jsonData);
        var _goodsNo = document.payForm.Moid.value;
        var _goodsName = document.payForm.GoodsName.value;
        var _totalPrice = document.payForm.Amt.value;
        var _taxFeeAmt = document.payForm.TaxFreeAmt.value;

        var query = "goodsno=" + _goodsNo + "&goodsname=" + _goodsName + "&totalprice=" + _totalPrice + "&taxfeeamt=" + _taxFeeAmt + "&pgdata=" + $("#ReqReserved").val();
        console.log(query);
        $.ajax({
            url: "/Course/PayKakaoReady",
            data: query,
            async: false,
            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
            dataType: "json",
            type: 'POST',
            success: function (data) {
                console.log("data : " + data);
                var _url = data.kurl;
                window.open(_url, 'kakaowin', 'toolbar=no,location=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=540,height=700,left=100,top=100');
            },
            error: function (request, status, error) {
                alert("카카오페이 결제 오류입니다.\n\n다시 시도하여 주십시오." + request.responseText);
            }
        });
	}

    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    // 회원정보 주소 가져오기
    //////////////////////////////////////////////////////////////////////////////////////////////////////////////
    function userAddress(m_id) {
        $.ajax({
            url: "/Services/OrderScript.asmx/GetUserAddress",
            data: "{ m_id:\"" + m_id + "\"}",
            async: false,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            type: 'POST',
            success: function (data) {
                var r = data.d;
                $('#tbOrdZipCode1').val(r.post);
                $('#tbOrdAddr1').val(r.address1);
                $('#tbOrdAddr2').val(r.address2);
            },
            error: function (request, status, error) {
                alert("다시 시도하여 주십시오." + request.responseText);
            }
        });
    }

    $(document).ready(function () {

        $('#cbAddress').click(function () {
            if ($('#cbAddress').prop('checked')) {
                userAddress('explorertea8');
            }
            else {
                $('#tbOrdZipCode1').val('');
                $('#tbOrdAddr1').val('');
                $('#tbOrdAddr2').val('');
            }
        });
    });
</script>

