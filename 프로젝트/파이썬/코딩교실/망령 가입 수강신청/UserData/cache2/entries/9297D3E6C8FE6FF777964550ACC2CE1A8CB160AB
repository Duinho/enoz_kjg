<script type="text/javascript">
    $(document).ready(function () {
		

	})

	$('.btn_zip').click(function () {
		$('#wrap_daum').children('div').css('min-width', '100px');
    });

    $('.IsNum').keyup(function () {
        $(this).val($(this).val().replace(/[^0-9]/gi, "")); //Л┬╚Л·░К╖▄ Л·┘К═╔Й╟─К┼╔
	});

    $('.IsKor').keyup(function (obj) {
        var reg = /[a-z0-9]|[ \[\]{}()<>?|`~!@#$%^&*-_+=,.;:\"'\\]/g;
        var v = $(this).val();
        var tag = "<span class='dot'>*</span> М∙°Й╦─К╖▄ Л·┘К═╔ Й╟─К┼╔М∙╘К▀┬К▀╓.";

        if (reg.test(v)) {
            $(this).val(v.replace(reg, ""));
            $('#tbMemNameAlert').html(tag);
        }
    });
</script>

