		if (!funcIsNull(obj.ddlAcaType, "М∙≥Й╣░ К═┬К╡╗Л²└", "select")) return;
		if (obj.ddlAcaType.value == "1") {
			if (!funcIsNull(obj.ddlAcaName1, "Л╢┬К⌠╠М∙≥Й╣░ Л²╢К╕└Л²└", "select")) return;
