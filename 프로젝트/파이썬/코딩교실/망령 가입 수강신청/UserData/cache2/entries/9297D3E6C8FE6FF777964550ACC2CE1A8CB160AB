		var pwd1 = document.frm.tbMemPass;
		var inNum = false;
		var inEng = false;
		var inSChar = false;
		var inRange = false;

		if (!pwd1.value) {
			alert("К╧└К╟─К╡┬М≤╦К╔╪ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
			pwd1.focus();
			return;
		} else {
			//К╧└К╟─К╡┬М≤╦ 8Л·░ Л²╢Л┐│Л²╦Л╖─ М≥∙Л²╦
			if (pwd1.value.length < 8) {
				alert("К╧└К╟─К╡┬М≤╦К┼■ 8Л·░ Л²╢Л┐│ Л·┘К═╔М∙╢Лё╪Л└╦Л ■.");
				pwd1.focus();
				return;
			}
			//Л┬╚Л·░М▐╛М∙╗Л≈╛К╤─ М≥∙Л²╦
			for (i = 0; i < pwd1.value.length; i++) {
				if ((pwd1.value.charCodeAt(i) >= 48) && (pwd1.value.charCodeAt(i) <= 57)) {
					inNum = true;
					break;
				}
			}
			//Л≤│К╛╦Л·░ М▐╛М∙╗Л≈╛К╤─ М≥∙Л²╦
			for (i = 0; i < pwd1.value.length; i++) {
				if (((pwd1.value.charCodeAt(i) >= 97) && (pwd1.value.charCodeAt(i) <= 122))) {
					inEng = true;
					break;
				}
			}

			if (pwd1.value.length < 10) {    //К╧└К╟─К╡┬М≤╦Й╟─ 10Л·░К╕╛ К╞╦К╖▄Л²╢К²╪К╘╢
				if (!(inNum && inEng)) {
					alert("К╧└К╟─К╡┬М≤╦К┼■ Л≤│К╛╦ Л├▄К╛╦Л·░Л≥─ Л┬╚Л·░К▐└ М∙╗Й╩≤ Л┌╛Л ╘М∙╢ Лё╪Л└╦Л ■.");
					return;
				}
			} else {
				if (inNum && inEng) {
					//return true;
				} else {
					alert("К╧└К╟─К╡┬М≤╦К┼■ Л≤│К╛╦ Л├▄К╛╦Л·░, Л┬╚Л·░К╔╪ Л└·Л√╢Л└° Л┌╛Л ╘М∙╢ Лё╪Л└╦Л ■.");
					return;
				}
			}
		}

		// ### К╧└К╟─К╡┬М≤╦ Л°═М ╗Л└╠ Л╡╢М│╛ ###
		if (obj.tbMemPass.value != obj.tbMemPass2.value) {
			alert("К╧└К╟─К╡┬М≤╦Й╟─ Л²╪Л╧≤М∙≤Л╖─ Л∙┼Л┼╣К▀┬К▀╓. К▐≥Л²╪М∙° К╧└К╟─К╡┬М≤╦К╔╪ Л·┘К═╔М∙╢ Лё╪Л└╦Л ■.");
			obj.tbMemPass2.focus();
			return;
		}
		// if (!funcIsNull(obj.tbMemEName, "Л≤│Л√╢ Л²╢К╕└Л²└", "insert")) return;
		if (!funcIsNull(obj.tbMobile1, "М°╢К▄─Л═└М≥■К╔╪", "insert")) return;
		if (!funcIsNull(obj.tbMobile2, "М°╢К▄─Л═└М≥■К╔╪", "insert")) return;
		if (!funcIsNull(obj.tbMobile3, "М°╢К▄─Л═└М≥■К╔╪", "insert")) return;

		if (obj.tbMobile1.value == "010"){
			if(obj.tbMobile2.value.length < 4){
				alert("М°╢К▄─Л═└М≥■ К╡┬М≤╦К╔╪ Л═∙М≥∙М·┬ Л·┘К═╔М∙╢Лё╪Л└╦Л ■");
				obj.tbMobile2.focus();
				return;
			}
			if(obj.tbMobile3.value.length < 4){
				alert("М°╢К▄─Л═└М≥■ К╡┬М≤╦К╔╪ Л═∙М≥∙М·┬ Л·┘К═╔М∙╢Лё╪Л└╦Л ■");
				obj.tbMobile3.focus();
				return;
			}
		}

		if (obj.tbMemEmail.value != "") {
			if (MailCheck(obj.tbMemEmail.value) == false) {
				alert('Л·≤К╙╩К░° Л²╢К╘■Л²╪Лё╪Л├▄ Л·┘К▀┬К▀╓.');
				obj.tbMemEmail.focus();
				return;
			}
		}

		if (!funcIsNull(obj.ddlBirthDay1, "Л╤°Л┐²К┘└К▐└К╔╪", "select")) return;
		if (!funcIsNull(obj.ddlBirthDay2, "Л┐²Л²╪Л²└", "select")) return;
		if (!funcIsNull(obj.ddlBirthDay3, "Л┐²Л²╪Л²└", "select")) return;

		if (!funcIsNull(obj.ddlSex, "Л·░Л▀═Л²≤ Л└╠КЁ└Л²└", "select")) return;

