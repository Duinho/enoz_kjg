			if (!funcIsNull(obj.tbPMobile1, "М∙≥К╤─К╙╗ Л≈╟К²╫Л╡≤К╔╪", "insert")) return;
			if (!funcIsNull(obj.tbPMobile2, "М∙≥К╤─К╙╗ Л≈╟К²╫Л╡≤К╔╪", "insert")) return;
			if (!funcIsNull(obj.tbPMobile3, "М∙≥К╤─К╙╗ Л≈╟К²╫Л╡≤К╔╪", "insert")) return;

