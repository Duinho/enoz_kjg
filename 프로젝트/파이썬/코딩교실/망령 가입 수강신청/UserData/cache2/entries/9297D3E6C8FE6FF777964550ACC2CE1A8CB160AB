				<script>
                    function funcMemCheck() {
                        var obj = $("#tbMemID").val();

                        if (obj == "") {
                            alert("Л∙└Л²╢К■■К╔╪ Л·┘К═╔М∙╢ Лё╪Л└╦Л ■.");
                            $('#tbMemID').focus();
                            return;
                        }

                        var RegExp = /^[A-za-z0-9]{4,15}$/g;  //Л≤│К╛╦ К▄─К╛╦Л·░ К≤░К┼■ Л├▄К╛╦Л·░ К≤░К┼■ Л┬╚Л·░, Й╦╦Л²╢К┼■ 4-15Л·░
                        if (!RegExp.test(obj)) {
                            alert('Л≤│К╛╦ К▄─/Л├▄К╛╦Л·░ К≤░К┼■ Л┬╚Л·░, Й╦╦Л²╢К┼■ 4-15Л·░К║° Л·┘К═╔М∙╢Лё╪Л└╦Л ■.');
                            $('#tbMemID').val("");
                            $('#tbMemID').focus();
                            return;
                        }

                        var filter = ['test', 'admin', 'super', 'insert', 'delete', 'dump'];
                        for (var i in filter) {
                            if (obj.match(filter[i])) {
                                alert('Й╦┬Л╖─Л√╢Й╟─ М▐╛М∙╗К░≤Л√╢Л·┬Л┼╣К▀┬К▀╓. - ' + filter[i]);
                                $('#tbMemID').val("");
                                $('#tbMemID').focus();
                                return;
                            }
                        }

                        $.ajax({
                            type: "post",
                            url: "/Services/AccountScript.asmx/GetMemberId",
                            data: "{ id:\"" + $("#tbMemID").val() + "\"}",
                            contentType: "application/json; charset=utf-8",
                            dataType: 'json',
                            error: function (request, status, error) {
                                console.log("code:" + request.status + ", message: " + request.responseText + ", error:" + error);
                                alert("Id Л║╟М ▄Л≈░ К╛╦Л═°Й╟─ К╟°Л┐²М√┬Л┼╣К▀┬К▀╓. Й╢─К╕╛Л·░Л≈░Й╡▄ К╛╦Л²≤ К╟■К·█К▀┬К▀╓.");
                            },
                            success: function (data) {
                                //$("#id_check").text(data);
                                if (data.d == "N") {
                                    $('#tbMemID').val("");
                                    $('#tbMemID').focus();
                                    $("#id_check").html("<font color='red'>Л╓▒КЁ╣К░° Л∙└Л²╢К■■Л·┘К▀┬К▀╓. К▀╓Л▀° М≥∙Л²╦М∙╢ Лё╪Л└╦Л ■.</font>");
                                } else {
                                    $('#hhdIdCheck').val('Y');
                                    $("#id_check").html("<font color='blue'>Л┌╛Л ╘ Й╟─К┼╔М∙° Л∙└Л²╢К■■Л·┘К▀┬К▀╓.</font>");
                                }
                            }
                        });
                    }
                </script>

			
