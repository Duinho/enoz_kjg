	function funcEnterID(i) {
		if (event.keyCode == 13 && i == 7) {
			chkUserId();
		}
	}

	// К⌠╠К║² / Л┬≤Л═∙
	function funcSave() {
		var obj = document.frm;
		var mode = document.getElementById("mode").value;
		//alert(mode);

		if (mode == "Create") {

			if (!funcIsNull(obj.tbMemName, "Л²╢К╕└Л²└", "insert")) return;
			if (!funcIsNull(obj.tbMemID, "Л∙└Л²╢К■■К╔╪", "insert")) return;
			if (obj.tbMemID.value.length < 4) {
				alert("Л┌╛Л ╘М∙═ Л∙└Л²╢К■■К╔╪ 4Л·░ Л²╢Л┐│Л°╪К║° Л·┘К═╔М∙≤Л└╦Л ■.");
				obj.tbMemID.focus();
				return;
			}
			if (obj.hhdIdCheck.value == "") {
				alert("Л∙└Л²╢К■■ Л╓▒КЁ╣Л╡╢М│╛К╔╪ М∙╢Лё╪Л└╦Л ■.");
				//chkUserId();
				return;
			}
			
			if(obj.tbMemName.value == obj.tbPName.value){
				alert("М∙≥Л┐²К╙┘ЙЁ╪ М∙≥К╤─К╙╗К╙┘Л²╢ К▐≥Л²╪М∙╘К▀┬К▀╓.");
				return;
			}

