			return;
		}
		// if (!funcIsNull(obj.tbMemEName, "мЃм–ґ мќґл¦„мќ„", "insert")) return;
		if (!funcIsNull(obj.tbMobile1, "нњґлЊЂм „н™”лҐј", "insert")) return;
		if (!funcIsNull(obj.tbMobile2, "нњґлЊЂм „н™”лҐј", "insert")) return;
		if (!funcIsNull(obj.tbMobile3, "нњґлЊЂм „н™”лҐј", "insert")) return;

		if (obj.tbMobile1.value == "010"){
			if(obj.tbMobile2.value.length < 4){
				alert("нњґлЊЂм „н™” лІ€нёлҐј м •н™•нћ€ мћ…л Ґн•ґмЈјм„ёмљ”");
