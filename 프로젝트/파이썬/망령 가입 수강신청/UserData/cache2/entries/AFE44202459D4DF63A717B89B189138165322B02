		var pwd1 = document.frm.tbMemPass;
		var inNum = false;
		var inEng = false;
		var inSChar = false;
		var inRange = false;

		if (!pwd1.value) {
			alert("л№„л°ЂлІ€нёлҐј мћ…л Ґн•ґмЈјм„ёмљ”.");
			pwd1.focus();
			return;
		} else {
			//л№„л°ЂлІ€нё 8мћђ мќґмѓЃмќём§Ђ н™•мќё
			if (pwd1.value.length < 8) {
				alert("л№„л°ЂлІ€нёлЉ” 8мћђ мќґмѓЃ мћ…л Ґн•ґмЈјм„ёмљ”.");
				pwd1.focus();
				return;
			}
			//м€«мћђнЏ¬н•Ём—¬л¶Ђ н™•мќё
			for (i = 0; i < pwd1.value.length; i++) {
				if ((pwd1.value.charCodeAt(i) >= 48) && (pwd1.value.charCodeAt(i) <= 57)) {
					inNum = true;
					break;
				}
			}
			//мЃл¬ёмћђ нЏ¬н•Ём—¬л¶Ђ н™•мќё
			for (i = 0; i < pwd1.value.length; i++) {
				if (((pwd1.value.charCodeAt(i) >= 97) && (pwd1.value.charCodeAt(i) <= 122))) {
					inEng = true;
					break;
				}
			}

			if (pwd1.value.length < 10) {    //л№„л°ЂлІ€нёк°Ђ 10мћђл¦¬ лЇёл§Њмќґлќјл©ґ
				if (!(inNum && inEng)) {
					alert("л№„л°ЂлІ€нёлЉ” мЃл¬ё м†Њл¬ёмћђм™Ђ м€«мћђлЏ„ н•Ёк» м‚¬мљ©н•ґ мЈјм„ёмљ”.");
					return;
				}
