	// л№„л°ЂлІ€нё н™•мќё мќјм№м—¬л¶Ђ
	function funcPassConfirm() {
		var obj = document.frm;

		if( obj.tbMemPass.value == obj.tbMemPass2.value ) {
			$("#tbPassConfirm").html("<font color='blue'>* л‘ђ л№„л°ЂлІ€нёк°Ђ лЏ™мќјн•©л‹€л‹¤.</font>");
		} else {
			$("#tbPassConfirm").html("<font color='red'>* л‘ђ л№„л°ЂлІ€нёк°Ђ лЏ™мќјн•м§Ђ м•ЉмЉµл‹€л‹¤.</font>");
		}
	}

	//н•™л…„л €лІЁ 2
	function funcAcaType() {
		var obj = document.frm;
		if (obj.ddlAcaType.value == 1) {
			$( '#aca_type1' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type1' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 2) {
			$( '#aca_type2' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type2' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 3) {
			$( '#aca_type3' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type3' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 4) {
			$( '#aca_type4' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type4' ).css( 'display', 'none');
		}
		else if (obj.ddlAcaType.value == 5) {
			$( '#aca_type5' ).css( 'display', 'block');
			$( '.aca_type' ).not( '#aca_type5' ).css( 'display', 'none');
		}
	}

	function funcAgeCheck(obj){
		var _year = parseInt(new Date().getFullYear());
		var _birthday1 = parseInt(obj.ddlBirthDay1.value);		
		var _age = _year - _birthday1;

		if(obj.ddlAcaType.value == "1"){
			if(!(_age >= 7 && _age <= 12)){
				alert("м¶њмѓќл…„лЏ„м™Ђ н•™кµђл €лІЁмќґ л§ћм§Ђ м•ЉмЉµл‹€л‹¤. н™•мќён•ґмЈјм„ёмљ”.");
				obj.ddlAcaType.focus();
				return false;				
			}
		}else if (obj.ddlAcaType.value == "2"){
			if(!(_age >= 13 && _age <= 15)){
				alert("м¶њмѓќл…„лЏ„м™Ђ н•™кµђл €лІЁмќґ л§ћм§Ђ м•ЉмЉµл‹€л‹¤. н™•мќён•ґмЈјм„ёмљ”.");
				obj.ddlAcaType.focus();
				return false;				
			}
		}else if (obj.ddlAcaType.value == "3"){
			if(!(_age >= 16 && _age <= 18)){
				alert("м¶њмѓќл…„лЏ„м™Ђ н•™кµђл €лІЁмќґ л§ћм§Ђ м•ЉмЉµл‹€л‹¤. н™•мќён•ґмЈјм„ёмљ”.");
				obj.ddlAcaType.focus();
				return false;				
			}
		}else if (obj.ddlAcaType.value == "4"){
			if(_age >= 7 && _age <= 18){
				alert("м¶њмѓќл…„лЏ„м™Ђ н•™кµђл €лІЁмќґ л§ћм§Ђ м•ЉмЉµл‹€л‹¤. н™•мќён•ґмЈјм„ёмљ”.");
				obj.ddlAcaType.focus();
				return false;				
			}
		}
		return true;
	}

</script>

<script>
	/* м•„мќґл”” : н•њкёЂ,нЉ№м€л¬ёмћђ м њк±° */
	function checkID(obj) {
		var RegExp1 = /^[a-zA-Z0-9]*$/;
		if (!RegExp1.test(obj.value)) {
			obj.value = obj.value.substring(0, obj.value.length - 1);
			$("#id_check").html("<font color='blue'>мµњм†Њ 4мћђ, мЃл¬ё+м€«мћђ н•нѓњлЎњ мћ…л Ґн•ґмЈјм„ёмљ”.</font>");
			$('#hhdIdCheck').val('');
		}
		else {
			$("#id_check").html("<span class='dot'>*</span> <span style='color:blue'>мµњм†Њ 4мћђ, мЃл¬ё+м€«мћђ н•нѓњлЎњ мћ…л Ґн•ґмЈјм„ёмљ”.</span>");
			$('#hhdIdCheck').val('');
		}
	}

	/* мЃм–ґмќґл¦„ : н•њкёЂ,нЉ№м€л¬ёмћђ м њк±° */
	function checkEngName(obj) {
		var RegExp1 = /^[a-zA-Z0-9]*$/;
		var RegExp2 = /[_]/gi;
		if ((RegExp1.test(obj.value) || RegExp2.test(obj.value)) == false) {
			obj.value = obj.value.substring(0, obj.value.length - 1);
			$("#EngName_check").html("<span style='color:blue'>мЃл¬ёмњјлЎњ мћ…л Ґн•ґмЈјм„ёмљ”.</span>");
		}
	}
</script>

<script>
    // ### RECAPTCHA ###
    function onSubmit(token) {
        //alert('thanks ');
        funcSave();
        //document.frm.submit();
    }

    function validate(event) {
        event.preventDefault();
        grecaptcha.execute();
    }

    function onload() {
        var element = document.getElementById('btnsubmit');
        element.onclick = validate;
    }
    // ### // RECAPTCHA ###
</script>

