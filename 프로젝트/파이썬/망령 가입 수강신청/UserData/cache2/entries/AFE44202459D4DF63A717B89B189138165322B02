    $('.IsKor').keyup(function (obj) {
        var reg = /[a-z0-9]|[ \[\]{}()<>?|`~!@#$%^&*-_+=,.;:\"'\\]/g;
        var v = $(this).val();
        var tag = "<span class='dot'>*</span> н•њкёЂл§Њ мћ…л Ґ к°ЂлЉҐн•©л‹€л‹¤.";

        if (reg.test(v)) {
            $(this).val(v.replace(reg, ""));
            $('#tbMemNameAlert').html(tag);
        }
    });
